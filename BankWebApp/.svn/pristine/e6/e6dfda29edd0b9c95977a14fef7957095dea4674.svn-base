/* ---------------------------- Menu --------------------------*/
#menu {
    margin:-10px;
    padding:0px;
    border:1px solid #e7e7e7;
    width:180px;
}

#menu ul{
    width: 200px;
    margin:-10px;
    padding:1px;
    border:1px solid #e7e7e7;
}

#menu li{
    display:marker;
    list-style:none;
    padding-bottom:1px;
}

#menuTitle{
	display:block;
    list-style:none;
    padding-bottom:1px;
    
    font-weight:bold;
    color:black;
    background-color: #e7e7e7;
	padding:5px;
	text-align: center;
	border-bottom: 1px solid white;
}

#menu li a{
    display:block;
    font-family:Tahoma, Geneva, sans-serif;
    font-size:15px;
    font-weight:bold;
    color:#475e88;
    text-shadow:#FFF 1px 1px 1px;
    text-decoration:none;
    background-color:#e7e7e7;
    padding:5px;
    border-left:10px solid #475e88;
}

#menu li a:hover{
    border-left:14px solid #475e88;
    background-color:white;
}

